<!DOCTYPE html>
<html>
<head>
  <title>Releaser</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<nav class="navbar navbar-default">
  <div class="container">
    <div class="navbar-header">
      <%= link_to 'Releaser', root_path, class: 'navbar-brand' %>
    </div>
    <ul class="nav navbar-nav">
      <li>
        <%= link_to 'Dashboard' %>
      </li>
      <li>
        <%= link_to 'Releases', releases_path %>
      </li>
      <li>
        <%= link_to 'Steps', steps_path %>
      </li>
      <li>
        <%= link_to 'Teams', teams_path %>
      </li>
      <li>
        <%= link_to 'Countries', countries_path %>
      </li>
    </ul>
  </div>
</nav>
<div class="container">
<%= yield %>
</div>
</body>
</html>